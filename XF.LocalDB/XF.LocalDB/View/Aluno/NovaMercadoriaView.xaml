<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="XF.LocalDB.View.Aluno.NovoAlunoView">
    <ContentPage.Content>
        <StackLayout Padding="20">
            <Entry x:Name="txtNomeMercadoria" Placeholder="NomeMercadoria"/>
            <Entry x:Name="txtpesoMercadoria" Placeholder="Peso"/>
            <Entry x:Name="txtNomeProduto" Placeholder="Produto"/>
            <Entry x:Name="txtEmail" Placeholder="Email"/>
            <Entry x:Name="txtNcm" Placeholder="NCM"/>
            <StackLayout>
                <Button x:Name="btnSalvar" Text="Salvar" Clicked="OnSalvar"/>
                <Button x:Name="btnCancelar" Text="Cancelar" Clicked="OnCancelar"/>
            </StackLayout>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>